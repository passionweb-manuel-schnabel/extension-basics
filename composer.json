{
  "name": "passionweb/extension-basics",
  "description": "Showcase of custom extension development in TYPO3",
  "type": "typo3-cms-extension",
  "minimum-stability": "stable",
  "version": "1.6.0",
  "homepage": "https://www.passionweb.de",
  "authors": [
    {
      "name": "Manuel Schnabel",
      "role": "Developer"
    }
  ],
  "require": {
    "typo3/cms-core": "^12.4 || ^13.4",
    "typo3/cms-backend": "^12.4 || ^13.4",
    "typo3/cms-beuser": "^12.4 || ^13.4"
  },
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Passionweb\\ExtensionBasics\\": "Classes/"
    }
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "extension_basics"
    }
  }
}
